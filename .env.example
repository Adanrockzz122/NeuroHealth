.# Example environment variables for neurohealth
# Copy this to `.env` and fill in secret values before running.

# Required API keys
GEMINI_API_KEY=
GITHUB_TOKEN=

# Model configuration (defaults used in code)
GEMINI_MODEL=gemini-2.0-flash
GITHUB_EMBEDDING_MODEL=openai/text-embedding-3-small
GITHUB_MODELS_EMBEDDING_ENDPOINT=https://models.github.ai/inference/embeddings

# File paths (relative to repository root)
NEUROHEALTH_KB_PATH=data/knowledge_base.json
NEUROHEALTH_FEEDBACK_PATH=data/feedback.jsonl
